{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "mini-css-extract-plugin": "^1.3.4",
    "svelte": "^3.31.2",
    "svelte-loader": "^3.0.0",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "ts-loader": "^8.0.4",
    "@tsconfig/svelte": "^1.0.10",
    "@types/node": "^14.11.1",
    "svelte-check": "^1.0.46",
    "svelte-preprocess": "^4.3.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3",
    "@smui-extra/accordion": "^6.0.0-beta.9",
    "@smui/button": "^6.0.0-beta.9",
    "@smui/card": "^6.0.0-beta.9",
    "@smui/checkbox": "^6.0.0-beta.9",
    "@smui/common": "^6.0.0-beta.9",
    "@smui/dialog": "^6.0.0-beta.9",
    "@smui/form-field": "^6.0.0-beta.9",
    "@smui/icon-button": "^6.0.0-beta.9",
    "@smui/layout-grid": "^6.0.0-beta.9",
    "@smui/linear-progress": "^6.0.0-beta.9",
    "@smui/paper": "^6.0.0-beta.9",
    "@smui/top-app-bar": "^6.0.0-beta.9",
    "smui-theme": "^6.0.0-beta.9"
  },
  "dependencies": {
    "svelte-dnd-action": "^0.9.14",
    "svelte-material-ui": "^6.0.0-beta.9",
    "svelte-spa-router": "^3.0.4"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "webpack serve --port 9000 --content-base public",
    "validate": "svelte-check",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/smui-dark.css -i src/theme/dark"
  }
}
